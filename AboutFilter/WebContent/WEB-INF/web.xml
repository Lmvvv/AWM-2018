<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>AboutFilter</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>/jsp/index.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>ReportFilter</filter-name>
    <filter-class>com.lh.filter.ReportFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ReportFilter</filter-name>
    <url-pattern>/jsp/index.jsp</url-pattern>
  </filter-mapping>
  
  <filter-mapping>
    <filter-name>ReportFilter</filter-name>
    <servlet-name>TodaysSpecialServlet</servlet-name>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>TodaysSpecialServlet</display-name>
    <servlet-name>TodaysSpecialServlet</servlet-name>
    <servlet-class>com.lh.casetwo.TodaysSpecialServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TodaysSpecialServlet</servlet-name>
    <url-pattern>/TodaysSpecialServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>NoInvokerServlet</display-name>
    <servlet-name>NoInvokerServlet</servlet-name>
    <servlet-class>com.lh.casetwo.NoInvokerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>NoInvokerServlet</servlet-name>
    <url-pattern>/servlet/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PageServle</display-name>
    <servlet-name>PageServle</servlet-name>
    <servlet-class>com.lh.fenye.PageServle</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PageServle</servlet-name>
    <url-pattern>/PageServle</url-pattern>
  </servlet-mapping>
</web-app>